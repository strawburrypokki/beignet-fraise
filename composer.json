{
    "require": {
        "symfony/console": "^5.0",
        "symfony/dependency-injection": "^5.0",
        "symfony/dotenv": "^5.0",
        "symfony/flex": "^1.18",
        "symfony/config": "^5.4",
        "symfony/yaml": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "require-dev": {
        "symfony/var-dumper": "^5.0",
        "friendsofphp/php-cs-fixer": "^3.6"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        }
    }
}
